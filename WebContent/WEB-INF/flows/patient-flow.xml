<?xml version="1.0" encoding="UTF-8"?>

<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<view-state id="patientsPage">

		<transition on="show-doctors-for-patient" to="doctors-list-for-patient"></transition>

	</view-state>



	<view-state id="doctors-list-for-patient">

		<transition on="choosen-doctor" to="doctors-schedule"></transition>

	</view-state>


	<view-state id="doctors-schedule">

		<transition on="enter-complaint" to="complaint-form"></transition>


	</view-state>

	<view-state id="complaint-form">
		<transition on="submit-complaint" to="complaint-submitted"></transition>
	</view-state>

	<end-state id="complaint-submitted"></end-state>

















</flow>